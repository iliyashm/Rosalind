   "source": [
    "def fibonacci(n, k):\n",
    "    if n <= 0:\n",
    "        return 0\n",
    "    elif n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        new, old = 1, 1\n",
    "        for i in range(2, n):\n",
    "            temp = new\n",
    "            new = k * old + new\n",
    "            old = temp\n",
    "        return new"
   ]
  },
